{
    "presets": ["babel-preset-expo"],
    "env": {
      "development": {
        "plugins": 
          ["@babel/plugin-transform-react-jsx-source",
            ["module-resolver",{
                "root": ["./src"],
                "alias": {
                  "@src": "./src",
                  "@pages": "./src/pages",
                  "@themeStyles": "./src/config/styles/theme",
                  "@components": "./src/components"
                }
              }
            ]
          ]
        }
      }
    }